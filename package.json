{
  "name": "ReactNSass2",
  "version": "1.0.0",
  "description": "Second React and Sass Boilerplate",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/scss ./src/scss -o ./public/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/scss ./src/scss -o ./public/css --watch --recursive",
    "start-js": "react-scripts start",
    "dev": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start": "node server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gio-aguirre19/ReactNSass2.git"
  },
  "bugs": {
    "url": "https://github.com/Gio-aguirre19/ReactNSass2/issues"
  },
  "homepage": "https://github.com/Gio-aguirre19/ReactNSass2#readme"
}
